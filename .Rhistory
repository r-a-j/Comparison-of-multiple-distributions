library(gplots)
library(ggplot2)
library(FSA)
#df <- read.csv(file = 'C:/Users/amrit/Documents/Courses/ICS/2/rent_index_99.csv')
df <- read.csv(file = 'G:/Study/TU Dortmund/Introductory Case Studies/Report 2/References/rent_index_99.csv')
df$rent_sqmPrice <- with(df, net.rent/living.area)
nan <-sapply(df, function(y) sum(length(which(is.na(y)))))
nan <- data.frame(nan)
NT <- sum(is.na(df))
print(NT)
Average <- df[ which(df$quality.of.location=='1'), ]
Good <- df[ which(df$quality.of.location=='2'), ]
Top <- df[ which(df$quality.of.location=='3'), ]
summary(Average$rent_sqmPrice)
nrow(Average)
var(Average$rent_sqmPrice)
summary(Good$rent_sqmPrice)
nrow(Good)
var(Good$rent_sqmPrice)
summary(Top$rent_sqmPrice)
nrow(Top)
var(Top$rent_sqmPrice)
# Assumptions check
#Normality check
hist(Average$rent_sqmPrice, prob = TRUE)
hist(Good$rent_sqmPrice, prob = TRUE)
hist(Top$rent_sqmPrice, prob = TRUE)
qqnorm(Average$rent_sqmPrice, pch = 1, frame = FALSE, main = "Q-Q Plot for Average Loc", cex.main=1.5,cex.lab=1)
qqline(Average$rent_sqmPrice, col = "steelblue", lwd = 2)
qqnorm(Good$rent_sqmPrice, pch = 1, frame = FALSE, main = "Q-Q Plot for Good Loc", cex.main=1.5,cex.lab=1)
qqline(Good$rent_sqmPrice, col = "steelblue", lwd = 2)
qqnorm(Top$rent_sqmPrice, pch = 1, frame = FALSE, main = "Q-Q Plot for Top Loc",cex.main=1.5, cex.lab=1)
qqline(Top$rent_sqmPrice, col = "steelblue", lwd = 2)
#Variance check
boxplot(rent_sqmPrice ~ quality.of.location, data = df, col = "white",  horizontal = TRUE, names=c("average","good","top"),cex.main=1.5, cex.lab=1)
stripchart(rent_sqmPrice ~ quality.of.location, data = df,method = "jitter",pch = 19,col = 2:4,add = TRUE)
# Non-parametric method
kruskal.test(rent_sqmPrice ~ quality.of.location, data = df)
# Without Bonferroni
pairwise.wilcox.test(df$rent_sqmPrice, df$quality.of.location, p.adjust.method = "none")
#With Bonferrooni
pairwise.wilcox.test(df$rent_sqmPrice, df$quality.of.location, p.adjust.method = "bonf")
